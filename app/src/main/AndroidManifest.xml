<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pablo.pruebasauthproyecto">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Usuarios.RegistroUsuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Navegador"
            android:label="@string/title_activity_navegador"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.CrearProyectos.CrearProyecto"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activities.CrearProyectos.AddRecursos"
            android:parentActivityName=".Activities.Navegador"
            android:label="@string/title_activity_add_recursos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.VerProyectos.FichaProyecto"
            android:parentActivityName=".Activities.Navegador"
            android:label="@string/title_activity_ficha_proyecto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Reproductores.AudioPlayer" />
        <activity android:name=".Reproductores.AudioPlayerUsuario" />
        <activity android:name=".Reproductores.VideoTubePlayer" />
        <activity android:name=".Reproductores.VideoPlayer" />
        <activity android:name=".Activities.MisProyectos"></activity>
    </application>

</manifest>